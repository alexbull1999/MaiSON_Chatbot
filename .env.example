# Example environment variables
DATABASE_URL=sqlite:///./chatbot.db
SECRET_KEY=your-secret-key

# Database Configuration
# For local development (using Docker)
AZURE_POSTGRES_HOST=db
AZURE_POSTGRES_USER=pgadmin
AZURE_POSTGRES_PASSWORD=dev_password_here
AZURE_POSTGRES_DB=postgres
AZURE_POSTGRES_PORT=5432

# For production (using Azure PostgreSQL)
# AZURE_POSTGRES_HOST=maisonchatbot-postgres.postgres.database.azure.com
# AZURE_POSTGRES_USER=your_azure_db_user
# AZURE_POSTGRES_PASSWORD=your_azure_db_password
# AZURE_POSTGRES_DB=your_azure_db_name
# AZURE_POSTGRES_PORT=5432

# LLM API Keys (using dummy values for testing)
OPENAI_API_KEY=sk-dummy-key
ANTHROPIC_API_KEY=sk-ant-dummy-key
GOOGLE_API_KEY=dummy-key

# External Service URLs (for local testing)
AUTH_SERVICE_URL=http://localhost:8001
PROPERTY_SERVICE_URL=http://localhost:8002
AVAILABILITY_SERVICE_URL=http://localhost:8003

# Service Authentication
SERVICE_SECRET_KEY=dev-secret-key

# API Keys
ZOOPLA_API_KEY=your_zoopla_api_key_here
PROPERTY_DATA_API_KEY=your_property_data_api_key_here
POLICE_UK_API_KEY=your_police_uk_api_key_here
ONS_API_KEY=your_ons_api_key_here

# API Endpoints (optional - will use defaults if not set)
ZOOPLA_API_BASE_URL=https://api.zoopla.co.uk/api/v1
PROPERTY_DATA_API_BASE_URL=https://api.propertydata.co.uk/v1
POLICE_UK_API_BASE_URL=https://data.police.uk/api
ONS_API_BASE_URL=https://api.ons.gov.uk

# Cache Settings (optional)
CACHE_TTL=3600
MAX_CACHE_ITEMS=1000

# Rate Limiting (optional)
ZOOPLA_RATE_LIMIT=100
OSM_RATE_LIMIT=2

# Search Radius in meters (optional)
DEFAULT_SEARCH_RADIUS=1000  # in meters

# Default search radius for location-based queries
DEFAULT_SEARCH_RADIUS=1000  # in meters

# Police UK API base URL
POLICE_UK_API_BASE_URL=https://data.police.uk/api

# ONS API base URL
ONS_API_BASE_URL=https://api.ons.gov.uk 